<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.mapper.MoneyMapper">
    <select id="SelectById" resultType="com.example.domain.money">
        select * from donation where id = #{id};
    </select>
    <update id="Update">
        update donation set current = #{update_current} where id = #{id};
    </update>
    <select id="Select_All" resultType="com.example.domain.money">
        select * from donation limit #{pageNum},#{pageSize};
    </select>
</mapper>